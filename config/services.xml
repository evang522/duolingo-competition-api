<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <imports>
        <import resource="services/command.xml"/>
        <import resource="services/services_bus.xml"/>
        <import resource="services/common.xml"/>
        <import resource="services/console_command.xml"/>
        <import resource="services/duolingo.xml"/>
        <import resource="services/host.xml"/>
        <import resource="services/competition.xml"/>
        <import resource="services/controller.xml"/>
        <import resource="services/user.xml"/>
    </imports>

    <services>
        <defaults autowire="true" autoconfigure="true" public="false"/>
    </services>
</container>
