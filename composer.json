{
  "type": "project",
  "name": "evang522/duolingo",
  "license": "proprietary",
  "require": {
    "php": "~7.4.9",
    "ext-ctype": "*",
    "ext-iconv": "*",
    "ext-json": "*",
    "doctrine/doctrine-bundle": "^2.2",
    "doctrine/doctrine-fixtures-bundle": "^3.3",
    "easycorp/easyadmin-bundle": "3.2.7",
    "friendsofsymfony/rest-bundle": "^2.7",
    "nelmio/cors-bundle": "^2.0",
    "php-http/httplug-bundle": "^1.18.0",
    "ramsey/uuid": "^3.8",
    "ramsey/uuid-doctrine": "^1.5",
    "swagger-api/swagger-ui": "^3.23",
    "symfony/asset": "4.4.0",
    "symfony/console": "^4.3",
    "symfony/dotenv": "^4.3",
    "symfony/flex": "^1.4",
    "symfony/framework-bundle": "^4.3",
    "symfony/http-client": "^4.3",
    "symfony/messenger": "^4.3",
    "symfony/monolog-bundle": "^3.5",
    "symfony/orm-pack": "^1.0",
    "symfony/profiler-pack": "1.0.*",
    "symfony/validator": "^4.3",
    "symfony/yaml": "4.3.*",
    "twig/extensions": ">=1.4 <2.0.0",
    "twig/twig": "v2.*",
    "webmozart/assert": "^1.9"
  },
  "require-dev": {
    "doctrine/coding-standard": "^8.1",
    "phpstan/phpstan": "^0.12.42",
    "phpstan/phpstan-doctrine": "^0.12.19",
    "phpunit/phpunit": "8.5.*",
    "roave/security-advisories": "dev-master",
    "squizlabs/php_codesniffer": "^3.5",
    "symfony/maker-bundle": "^1.29"
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/",
      "App\\DataFixtures\\": "fixtures/DataFixtures"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "replace": {
    "paragonie/random_compat": "2.*",
    "symfony/polyfill-ctype": "*",
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php72": "*",
    "symfony/polyfill-php71": "*",
    "symfony/polyfill-php70": "*",
    "symfony/polyfill-php56": "*"
  },
  "scripts": {
    "test": [
      "phpunit --testdox --color=always"
    ],
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ],
    "cs-check": "phpcs -p",
    "cs-fix": "phpcbf -p",
    "anal": "phpstan analyse"
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "description": "A Project"
}
